{
  "name": "functions-do",
  "version": "0.1.0",
  "description": "Functions.do - Multi-language serverless platform on Cloudflare Workers",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:cli": "vitest run --config vitest.node.config.ts",
    "test:all": "vitest run && vitest run --config vitest.node.config.ts",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.8.0",
    "@cloudflare/workers-types": "^4.20250109.0",
    "@types/node": "^22.0.0",
    "pyodide": "^0.29.1",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0",
    "wrangler": "^3.105.0"
  },
  "dependencies": {
    "ai-evaluate": "^2.1.1",
    "esbuild": "^0.27.2"
  }
}
